{
  "name": "@mikbin80/algorithmsts",
  "version": "0.0.1",
  "description": "",
  "private": false,
  "keywords": [],
  "main": "dist/algorithmsts.cjs.js",
  "module": "dist/algorithmsts.esm.js",
  "typings": "dist/types/algorithmsts.d.ts",
  "types": "dist/types/algorithmsts.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/algorithmsts.d.ts",
      "import": "./dist/algorithmsts.esm.js",
      "require": "./dist/algorithmsts.min.cjs"
    },
    "./core": {
      "types": "./dist/types/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./data-structures": {
      "types": "./dist/types/data-structures/index.d.ts",
      "import": "./dist/data-structures/index.js",
      "require": "./dist/data-structures/index.cjs"
    },
    "./algorithms": {
      "types": "./dist/types/algorithms/index.d.ts",
      "import": "./dist/algorithms/index.js",
      "require": "./dist/algorithms/index.cjs"
    },
    "./graphs": {
      "types": "./dist/types/graphs/index.d.ts",
      "import": "./dist/graphs/index.js",
      "require": "./dist/graphs/index.cjs"
    },
    "./performance": {
      "types": "./dist/types/performance/index.d.ts",
      "import": "./dist/performance/index.js",
      "require": "./dist/performance/index.cjs"
    },
    "./types": {
      "types": "./dist/types/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./compatibility": {
      "types": "./dist/types/compatibility/index.d.ts",
      "import": "./dist/compatibility/index.js",
      "require": "./dist/compatibility/index.cjs"
    },
    "./interfaces": {
      "types": "./dist/types/interfaces.d.ts",
      "import": "./dist/interfaces.js",
      "require": "./dist/interfaces.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Michele Bini <mikbin80@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MikBin/algorithmsts"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "scripts": {
    "lint": "eslint src/",
    "watch": "rollup -w -c rollup.config.ts --configPlugin typescript --environment BUILD:development",
    "build": "tsc -p tsconfig.json && node tools/fast-build-stub.mjs",
    "docs": "npm run generate-docs",
    "generate-docs": "echo generate-docs",
    "build:docs": "node tools/build-and-docs.ts",
    "test": "vitest",
    "test:prod": "vitest run --coverage",
    "bench": "vitest bench",
    "prepare": "husky",
    "v-pub-tarball": "npm publish --dry-run",
    "release": "release-it",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@release-it/conventional-changelog": "^9.0.4",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/d3": "^7.4.3",
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/node": "^22.10.3",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitest/coverage-istanbul": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "release-it": "^17.11.0",
    "rollup": "^4.29.1",
    "rollup-plugin-delete": "^2.1.0",
    "semantic-release": "^24.2.0",
    "typedoc": "^0.28.14",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "jsdom": "^27.0.1"
  },
  "dependencies": {
    "d3": "^7.9.0"
  }
}
